  "version": 2,
  "redirects": [
    {
      "source": "/",
      "destination": "/home.html",
      "permanent": true
    }
  ],
  "builds": [
    {
      "src": "api/fetchUrls.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
